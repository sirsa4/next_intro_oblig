// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// TODO: Delete
model User {
  id         Int     @id @default(autoincrement())
  first_name String
  last_name  String
  email      String? @unique
}
